tblastn_vdb: 
	${MAKE} ${MFLAGS} -f Makefile.tblastn_vdb_app

blast_vdb_cmd:
	${MAKE} ${MFLAGS} -f Makefile.blast_vdb_cmd_app

blast_formatter_vdb:
	${MAKE} ${MFLAGS} -f Makefile.blast_formatter_vdb_app

