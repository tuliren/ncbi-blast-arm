APP_PROJ = blastn_vdb tblastn_vdb blast_vdb_cmd blast_formatter_vdb

REQUIRES = objects algo app VDB

srcdir = @srcdir@
include @builddir@/Makefile.meta

.PHONY: all $(APP_PROJ)

blastn_vdb: 
